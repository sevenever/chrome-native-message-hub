        "scripts": [ "background.js" ]
    },
    "manifest_version": 2,
    "applications": {
        "gecko": {
            "id": "chrome-native-message-hub@sevenever.com"
        }
    },
    "permissions": [
        "nativeMessaging"
    ],
    "version": "0.0.1"
}
